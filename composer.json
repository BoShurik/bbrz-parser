{
    "name": "boshurik/bbrz-parser",
    "description": "Parser for Blood Bowl 2 replays",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BoShurik",
            "email": "boshurik@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-simplexml": "*",
        "ext-zip": "*",
        "symfony/filesystem": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2",
        "symfony/var-exporter": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "BoShurik\\BloodBowl\\Replay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoShurik\\BloodBowl\\Replay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
